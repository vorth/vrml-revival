    url [ "javascript:
       function isActive(eventValue) {
          if (eventValue == true) {
            if (first == true) {
              switchvalue = 1;
              first = false;
              }
            if (switchvalue == 0) {
              switchvalue = 1;
              transp = 0;
              }
