   eventIn SFBool isActive
   eventOut SFInt32 switchvalue
   eventOut SFFloat transp
]
{ Script {
  eventIn SFBool isActive IS isActive
  eventOut SFInt32 switchvalue IS switchvalue
  eventOut SFFloat transp IS transp
