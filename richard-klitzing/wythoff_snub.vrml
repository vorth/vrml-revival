PROTO AP01 [
	field SFColor IRO 1 0 0
	field SFFloat TR 0
]
{
	Appearance {
		material Material {
			diffuseColor IS IRO 
			transparency IS TR
		}
	}
}

PROTO BOX01 [
	field SFVec3f SIZE .14 .14 1
	field SFVec3f POS 0 0 0
	exposedField SFColor IRO 0 0 0
	exposedField SFFloat TR 0
	exposedField MFString TEXT ""
	exposedField SFVec3f TEXTPOS 0 0 .1
]
{
	Transform {
		translation IS POS 
		scale IS SIZE 
		children [
			Shape {
				appearance Appearance {
					material Material {
						emissiveColor IS IRO 
						diffuseColor 0 0 0 
						transparency IS TR
					}
				}
				geometry IndexedFaceSet { 
					coord Coordinate { 
						point [
							.5 .5 0 
							-.5 .5 0 
							-.5 -.5 0 
							.5 -.5 0
						] 
					} 
					coordIndex [
						0 1 2 3
					] 
				} 
			}
			Transform {
				translation IS TEXTPOS
				scale .27 1.1 1
				children [
					Shape {
						geometry Text {
							fontStyle FontStyle {
								size .55
								family "SANS"
								justify ["MIDDLE", "MIDDLE"]
							}
							string IS TEXT
						}
					}
				]
			}
		]
	}
}

