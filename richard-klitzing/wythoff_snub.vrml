DEF SC001 Script{
	eventIn SFTime vp_touch
	eventIn SFVec3f button_move
	eventIn SFTime clicked
	eventIn SFVec3f lev6_move
	eventIn SFVec3f stick2triang
	eventIn SFTime acr_touch
	eventIn SFTime snub_touch
	eventIn SFTime grid_touch
	field SFFloat PI 0
	field SFFloat phi12 0
	field SFFloat chi20 0
	field SFInt32 current 0
	field SFFloat current_vp 0
	field MFRotation rotate_on [
		0 0 1 0 
		0 0 1 1.5707963267948966192313216916398 
		0 0 1 3.1415926535897932384626433832795 
		0 0 1 4.7123889803846898576939650749193 
		0 0 1 6.283185307179586476925286766559
	]
	field MFRotation rotate_off [
		0 0 1 0 
		0 0 1 0 
		0 0 1 0 
		0 0 1 0 
		0 0 1 0
	]
	field SFRotation ss_reset 0 0 1 0
	field MFInt32 color_index [ 0 0 0 1 1 1 ]
	field SFFloat e_rotate 0
	field SFFloat e_distance .001
	field SFInt32 grid_choice 0
	field SFInt32 snub_var 0
	eventOut SFVec3f kyori
	eventOut SFVec3f scale
	eventOut SFRotation kaiten
	eventOut SFInt32 choice
	eventOut MFRotation rot_on_off
	eventOut SFRotation vp_rotation
	eventOut SFRotation reset_ss
	eventOut SFVec3f trackball_scale
	eventOut SFRotation rotation_changed
	eventOut SFVec3f distance
	eventOut SFVec3f all_scale
	eventOut SFVec3f tr_trans
	eventOut SFVec3f bt_trans
	eventOut SFVec3f tr_pa
	eventOut SFVec3f tr_pb
	eventOut MFVec3f trig01
	eventOut MFVec3f trig02
	eventOut MFVec3f trig03
	eventOut MFVec3f trig04
	eventOut MFVec3f trig05
	eventOut MFVec3f trig06
	eventOut MFString text
	eventOut SFVec3f init
	eventOut MFVec3f squ01
	eventOut SFFloat acryl
	eventOut MFString acr_txt
	eventOut MFVec3f v_snub
	eventOut SFInt32 snub_choice
	eventOut MFString snub_txt
	eventOut SFInt32 grd_on_off
	eventOut MFString grd_txt
	url "javascript:
	function initialize (){
		acr_val=0;
		M5=Math.sqrt(5);
		M3=Math.sqrt(3);
		M2=Math.sqrt(2);
		PI=Math.PI;
		phi12=Math.asin(Math.sqrt(10+2*M5)/(2*M5));
		phi20sup=PI/2-Math.acos((M5-1)/2/Math.sqrt(3)); //90-phi20
		chi20=Math.asin(Math.sqrt((5+2*M5)/15));
		ztoy=new SFRotation(1,1,1,PI*2/3);
		transformer();
	}
	function vp_touch (value) {
		if (current_vp==0) {
			/* rotating -> n-fold */
			/* all symmetries */
			current_vp=1;
			vp_rotation=new SFRotation(0,1,0,PI/2);
			rot_on_off=rotate_off;
		}
		else if(current_vp==1){
			/* n-fold -> 3-fold */
			current_vp=2;
			if(current==0){
				/* [3,5] */
				alpha=Math.atan(2);
				beta=Math.acos(-Math.cos(alpha)/Math.sin(alpha/2))/2;
				gamma=Math.asin(2/3)/4;
				delta=Math.atan(Math.sin(gamma)*Math.tan(beta));
				epsi=Math.acos(Math.cos(delta)*Math.cos(gamma));
				zeta=Math.atan(Math.tan(epsi)*Math.sin(beta));
				eta=Math.asin(Math.sin(beta)*Math.sin(gamma)/Math.sin(delta));
				vp_rotation=new SFRotation(-Math.sin(zeta),-Math.cos(zeta)*Math.sin(gamma),Math.cos(zeta)*Math.cos(gamma),-2*eta);
			}
			else if(current==1){
				/* [3,4] */
				C225=Math.cos(PI/8);
				S225=Math.sin(PI/8);
				Cgamma=-C225*C225+S225*S225/M3;
				Sgamma=Math.sqrt(1-Cgamma*Cgamma);
				Ca=C225*(1+Cgamma)/(S225*Sgamma);
				Sa=Math.sqrt(1-Ca*Ca);
				Sh=Sa*C225;
				Ch=Math.sqrt(1-Sh*Sh);
				Cb=Ca/Ch;
				Sb=Math.sqrt(1-Cb*Cb);
				vp_rotation=new SFRotation(-Ch*Sb,Sh,Ch*Cb,Math.acos(Cgamma));
			}
			else if(current==2){
				/* [3,3] */
				vp_rotation=new SFRotation(0,1,0,-PI/2);
			}
		}
		else if(current_vp==2){
			/* 3-fold -> 2-fold */
			current_vp=3;
			if(current==0){
				/* [3,5] */
				vp_rotation=new SFRotation(0,0,1,phi12+PI/2);
			}
			else if(current==1){
				/* [3,4] */
				vp_rotation=new SFRotation(0,1,0,PI/4);
			}
			else if(current==2){
				/* [3,3] */
				alpha=Math.atan(1/M2)/2
				beta=Math.acos(Math.cos(alpha)/M2)
				gamma=Math.asin(Math.sin(alpha)/(M2*Math.sin(beta)))
				vp_rotation=new SFRotation(Math.cos(gamma)*Math.cos(alpha),-Math.sin(gamma),Math.cos(gamma)*Math.sin(alpha),2*beta);
			}
		}
		else {
			/* ? -> rotating */
			current_vp=0;
			vp_rotation=new SFRotation(1,0,0,-0.8);
			rot_on_off=rotate_on;
		}
		reset_ss=ss_reset;
	}
	function grid_touch (value, time) {
		grid_choice=(grid_choice+1) % 2;
		if(grid_choice==0){
			grd_txt=new MFString('hide grid');
		}
		else if(grid_choice==1){
			grd_txt=new MFString('show grid');
		}
		grd_on_off=grid_choice;
	}
	function snub_touch (value, time) {
		snub_var=(snub_var+1) % 2;
		if(snub_var==0){
			snub_txt=new MFString('v semiation');
		}
		else if(snub_var==1){
			snub_txt=new MFString('no semiation');
		}
		snub_choice=snub_var;
	}
	function stick2triang (v) {
		x=v[0]-1;
		y=1-v[1]/M3;
		if (x < -y) {
			x=-y;
		}
		else if (x > y) {
			x=y;
		}
		tr_trans=new SFVec3f(x,v[1]+1-M3,v[2]);
		yy=1-y;
		xx=(1+x+yy)/2; //planar
		if(current==0) {
			/* [3,5] */
			theta=Math.atan(2)/2; //5-2
			phi=Math.asin(2/3)/2; //2-3
			sth=Math.sin(theta);
			cth=Math.cos(theta);
			sph=Math.sin(phi);
			cph=Math.cos(phi);
			xxx=(cth-1)*xx+cth*(cph-1)*yy+1;
			yyy=sth*(xx+(cph-1)*yy);
			zzz=sph*yy;
			nn=xxx*xxx+yyy*yyy+zzz*zzz;
			n=Math.sqrt(nn);
			tr_pa=new SFVec3f(xxx/n, yyy/n, zzz/n);
			tr_pb=new SFVec3f(xxx/n, yyy/n, -zzz/n);
			tr_pc=new SFVec3f(
				tr_pb[0],
				Math.cos(2*PI/5)*tr_pb[1]-Math.sin(2*PI/5)*tr_pb[2],
				Math.sin(2*PI/5)*tr_pb[1]+Math.cos(2*PI/5)*tr_pb[2]
			);
		}
		else if(current==1) {
			/* [3,4] */
			theta=PI/4; //4-2
			phi=Math.acos(1/3)/2; //2-3
			sth=Math.sin(theta);
			cth=Math.cos(theta);
			sph=Math.sin(phi);
			cph=Math.cos(phi);
			xxx=(cth-1)*xx+cth*(cph-1)*yy+1;
			yyy=sth*(xx+(cph-1)*yy);
			zzz=sph*yy;
			nn=xxx*xxx+yyy*yyy+zzz*zzz;
			n=Math.sqrt(nn);
			tr_pa=new SFVec3f(xxx/n, yyy/n, zzz/n);
			tr_pb=new SFVec3f(xxx/n, yyy/n, -zzz/n);
			tr_pc=new SFVec3f(
				tr_pb[0],
				Math.cos(2*PI/4)*tr_pb[1]-Math.sin(2*PI/4)*tr_pb[2],
				Math.sin(2*PI/4)*tr_pb[1]+Math.cos(2*PI/4)*tr_pb[2]
			);
		}
		else if(current==2){
			/* [3,3] */
			theta=Math.atan(M2); //3-2
			phi=Math.atan(M2); //2-3
			sth=Math.sin(theta);
			cth=Math.cos(theta);
			sph=Math.sin(phi);
			cph=Math.cos(phi);
			xxx=(cth-1)*xx+cth*(cph-1)*yy+1;
			yyy=sth*(xx+(cph-1)*yy);
			zzz=sph*yy;
			nn=xxx*xxx+yyy*yyy+zzz*zzz;
			n=Math.sqrt(nn);
			tr_pa=new SFVec3f(xxx/n, yyy/n, zzz/n);
			tr_pb=new SFVec3f(xxx/n, yyy/n, -zzz/n);
			tr_pc=new SFVec3f(
				tr_pb[0],
				Math.cos(2*PI/3)*tr_pb[1]-Math.sin(2*PI/3)*tr_pb[2],
				Math.sin(2*PI/3)*tr_pb[1]+Math.cos(2*PI/3)*tr_pb[2]
			);
		}
		cent01=new SFVec3f(xxx/n, 0, 0);
		trig01=new MFVec3f(cent01, tr_pa, tr_pb);
		trig02=new MFVec3f(cent01, tr_pa, tr_pc);
		squc_x=(xxx*cth+yyy*sth)*cth/n;
		squc_y=(xxx*cth+yyy*sth)*sth/n;
		sq_pa=new SFVec3f(2*squc_x-xxx/n, 2*squc_y-yyy/n, -zzz/n);
		sq_pb=new SFVec3f(2*squc_x-xxx/n, 2*squc_y-yyy/n, zzz/n);
		squ01=new MFVec3f(tr_pa, tr_pb, sq_pa, sq_pb);
		cP3=(xxx*cth*cph+yyy*sth*cph+zzz*sph)/n;
		cent03=new SFVec3f(cP3*cth*cph, cP3*sth*cph, cP3*sph);
		trig03=new MFVec3f(cent03, tr_pa, sq_pb);
		trig04=new MFVec3f(cent03, tr_pa, tr_pc);
		v_snub=new MFVec3f(tr_pb, tr_pc, sq_pb);
		trig05=new MFVec3f(cent01, tr_pb, tr_pc);
		trig06=new MFVec3f(cent03, sq_pb, tr_pc);
	}
	function button_move (v) {
		bt_trans=new SFVec3f(v[0],v[1],v[2]);
	}
	function acr_touch (value, time) {
		acr_val = (acr_val+1) % 2;
		acryl = 0.7*acr_val;
		if(acr_val==0){
			acr_txt=new MFString('solid >> transp');
		}
		else if(acr_val==1){
			acr_txt=new MFString('transp >> solid');
		}
	}
	function clicked(value, time) {
		current = (current+1) % 3;
		choice = current;
		init[0]=0;
		init[1]=0;
		current_vp=3;
		vp_touch (3); 
		if(current==0){
			text=new MFString('[3,5] >> [3,4]')
		}
		else if(current==1){
			text=new MFString('[3,4] >> [3,3]')
		}
		else if(current==2){
			text=new MFString('[3,3] >> [3,5]')			
		}
	}
	function lev6_move (value) {;
		distance=new SFVec3f(0,0,-value[1]*20);
	}
	function transformer () {
		trackball_rad=Math.sqrt(Math.pow(e_distance,2)+1)*1.1;
		all_scale=new SFVec3f(1,1,1);
		trackball_scale=new SFVec3f(trackball_rad,trackball_rad,trackball_rad);
		kyori=new SFVec3f(0,0,e_distance);
		kaiten=new SFRotation(0,0,1,e_rotate);
	}
	function eventsProcessed() {
		transformer();
	}"
}

